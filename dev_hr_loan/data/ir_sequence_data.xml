<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for hr.loan -->
        <record id="hr_loan_sequence" model="ir.sequence">
            <field name="name">Hr Loan Sequence</field>
            <field name="code">employee.loan</field>
            <field name="prefix">Loan/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for Skipp installment -->
        <record id="dev_skip_installment" model="ir.sequence">
            <field name="name">Skip installment Sequence</field>
            <field name="code">dev.skip.installment</field>
            <field name="prefix">SKP-INS/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for Loan Documents -->
        <record id="seq_loan_document" model="ir.sequence">
            <field name="name">LOAN/DOC</field>
            <field name="code">dev.loan.document</field>
            <field name="prefix">LOAN/DOC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
