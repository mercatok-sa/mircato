<odoo>
    <data noupdate="1">
        <record id="sequence_hr_petty_cash" model="ir.sequence">
            <field name="name">Petty Cash</field>
            <field name="code">petty.cash</field>
            <field name="prefix">PCSH/%(y)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>


        <record id="sequence_hr_petty_cash_adjustment" model="ir.sequence">
            <field name="name">Petty Cash adjustment</field>
            <field name="code">petty.cash.adj</field>
            <field name="prefix">PCSHADJ/%(y)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>